				for (var i=0;i<data.length;i++)
				{
					var html = "<table><tr><td>옵션</td><td>리뷰수</td></tr>";
					for(var item in data[i]){
						var total = data[i][item];
						
						
						
						
						var html += "</table>"
						
						
					}
					
					
				console.log((data[i] + "<br>"));
				}
		  	});
			
		});
		
	})
	
	
	
