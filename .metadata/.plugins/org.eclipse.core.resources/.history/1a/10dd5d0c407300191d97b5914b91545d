package org.springframework.samples.mvc.views;

import org.apache.logging.log4j.core.lookup.MainMapLookup;

public class TotalObject {
	double totalCount;
	double totalScore;
	double avg;
	
	
	public  getAvgScore() {
		if(totalCount == 0) {
			return 0;
		}
		return totalScore/totalCount;
		
	}
	
}
